  "assumptions": [
    "Solo performance is predictive of head-to-head capability",
    "Win rate is approximately determined by relative skill",
    "Two-player games have minimal meta-game effects",
    "Order of play (first vs second) has negligible effect"
  ],

  "limitations": [
    "Two-player dynamics may differ from 4-player",
    "Doesn't test adaptation to specific opponent",
    "Win rate may be noisy for closely-matched strategies"
  ],

  "hypotheses": [
    {
      "id": "H1",
      "null": "Optimal brain has 50% win rate against professor",
      "alternative": "Optimal brain has greater than 50% win rate against professor",
      "metric": "win_rate",
      "compareConditions": ["C01-optimal-vs-professor"],
      "direction": "greater",
      "expectedEffectSize": 0.3,
      "test": "chi-square",
      "alpha": 0.05,
      "power": 0.80,
      "rationale": "Pure EV should have slight edge over personality-influenced decisions"
    },
    {
      "id": "H2",
      "null": "Professor has 50% win rate against carmen",
