      "expectedEffectSize": 0.1,
      "test": "chi-square",
      "alpha": 0.05,
      "power": 0.80,
      "rationale": "Validates that first/second player has no systematic advantage"
    },
    {
      "id": "H4",
      "null": "Score differential in optimal vs random is zero",
      "alternative": "Score differential in optimal vs random is positive",
      "metric": "total_score",
      "compareConditions": ["C04-optimal-vs-random"],
      "direction": "greater",
      "expectedEffectSize": 2.5,
      "test": "t-test-paired",
      "alpha": 0.05,
      "power": 0.99,
      "rationale": "Optimal should dominate random; validates competitive matchup coding"
    }
